loadstring(game:HttpGet("https://raw.githubusercontent.com/SaulGoodmanScripter/Utilit/refs/heads/main/Loader_Ui"))()
-- Load library
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/SaulGoodmanScripter/library/refs/heads/main/The%20main%20code"))()

-- Services
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local VirtualUser = game:GetService("VirtualUser")
local UserInputService = game:GetService("UserInputService")
local StarterGui = game:GetService("StarterGui")
local TextChatService = game:GetService("TextChatService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local player = Players.LocalPlayer

-- ====== CONFIGURATION ======
local AutoFarmCFrame = CFrame.new(-280, 167, 341)
local LobbyCFrame    = CFrame.new(-296, 195, 370)
local GameAreaCFrame = CFrame.new(-104, 48, 11)

local TP_INTERVAL  = 0.6
local MAX_DISTANCE = 5
local FALL_OFFSET  = 5
local FLING_LIMIT  = 65

-- ====== STATE ======
local AutoFarm = false
local lastTP = 0
local character, hrp

-- ====== CREATE WINDOW ======
local Window = Library:Window({
    Title = "Natural disaster",
    Desc = "By Saul Hub",
    Theme = "Ocean",
    Config = {
        Keybind = Enum.KeyCode.RightControl,
        Size = UDim2.new(0, 550, 0, 350)
    },
    CloseUIButton = {
        Enabled = true,
        Text = "HIDE/SHOW"
    }
})

-- ====== CREATE TABS ======
local MainTab = Window:Tab({
    Title = "Main",
    Icon = "house"
})

local TeleportTab = Window:Tab({
    Title = "Teleport",
    Icon = "zap"
})

local SettingsTab = Window:Tab({
    Title = "Settings",
    Icon = "settings"
})

-- ====== ANTI AFK ======
player.Idled:Connect(function()
    VirtualUser:Button2Down(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
    task.wait(1)
    VirtualUser:Button2Up(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
end)

-- ====== CHARACTER HANDLER ======
local noclipConn
local function EnableNoclip()
    if noclipConn then return end
    noclipConn = RunService.Stepped:Connect(function()
        if character then
            for _,v in ipairs(character:GetDescendants()) do
                if v:IsA("BasePart") then
                    v.CanCollide = false
                end
            end
        end
    end)
end

local function ApplyCharacter(char)
    character = char
    hrp = char:WaitForChild("HumanoidRootPart", 5)
    task.wait()

    if hrp then
        hrp.AssemblyLinearVelocity = Vector3.zero
        hrp.AssemblyAngularVelocity = Vector3.zero
    end

    EnableNoclip()
end

player.CharacterAdded:Connect(ApplyCharacter)
if player.Character then ApplyCharacter(player.Character) end

-- ====== ANTI FLING ======
RunService.Heartbeat:Connect(function()
    if not hrp then return end
    if hrp.AssemblyLinearVelocity.Magnitude > FLING_LIMIT then
        hrp.AssemblyLinearVelocity = Vector3.zero
        hrp.AssemblyAngularVelocity = Vector3.zero
    end
end)

-- ====== AUTO FARM LOOP ======
RunService.Heartbeat:Connect(function()
    if not AutoFarm or not hrp then return end
    local now = tick()
    if now - lastTP >= TP_INTERVAL then
        local dist = (hrp.Position - AutoFarmCFrame.Position).Magnitude
        local fall = hrp.Position.Y < (AutoFarmCFrame.Position.Y - FALL_OFFSET)
        if dist > MAX_DISTANCE or fall then
            hrp.CFrame = AutoFarmCFrame
            lastTP = now
        end
    end
end)

-- ====== MAIN TAB ELEMENTS ======
MainTab:Section({
    Title = "Auto Farm"
})

local AutoFarmToggle = MainTab:Toggle({
    Title = "Auto Farm",
    Desc = "Automatically teleports to farm location",
    Value = false,
    Callback = function(value)
        AutoFarm = value
        if AutoFarm and hrp then 
            hrp.CFrame = AutoFarmCFrame
        end
    end
})

MainTab:Label({
    Title = "Status",
    Desc = AutoFarm and "ON - Farming..." or "OFF - Idle"
})

-- Create separator using Label with empty text
MainTab:Label({
    Title = "",
    Desc = ""
})

MainTab:Section({
    Title = "Features"
})

MainTab:Toggle({
    Title = "Noclip",
    Desc = "Walk through walls (Always ON)",
    Value = true,
    Callback = function(value)
        if value then
            EnableNoclip()
        else
            if noclipConn then
                noclipConn:Disconnect()
                noclipConn = nil
            end
        end
    end
})

MainTab:Toggle({
    Title = "Anti Fling",
    Desc = "Prevents getting flung",
    Value = true,
    Callback = function(value)
        -- Anti fling is always enabled in heartbeat loop
    end
})

MainTab:Toggle({
    Title = "Anti AFK",
    Desc = "Prevents being kicked for AFK",
    Value = true,
    Callback = function(value)
        -- Already enabled by default
    end
})

-- Create separator using Label with empty text
MainTab:Label({
    Title = "",
    Desc = ""
})

MainTab:Section({
    Title = "Scripts"
})

MainTab:Button({
    Title = "Infinite Yield",
    Desc = "Load Infinite Yield admin script",
    Image = "code",
    Callback = function()
        loadstring(game:HttpGet(
            "https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"
        ))()
        Window:Notify({
            Title = "Infinite Yield",
            Desc = "Loaded successfully!",
            Time = 3
        })
    end
})

-- ====== TELEPORT TAB ELEMENTS ======
TeleportTab:Section({
    Title = "Locations"
})

TeleportTab:Button({
    Title = "Teleport to Farm",
    Desc = "Teleport to auto farm location",
    Image = "zap",
    Callback = function()
        if hrp then
            hrp.CFrame = AutoFarmCFrame
            AutoFarm = false
            AutoFarmToggle:SetValue(false)
            Window:Notify({
                Title = "Teleport",
                Desc = "Teleported to farm location",
                Time = 2
            })
        end
    end
})

TeleportTab:Button({
    Title = "Teleport to Lobby",
    Desc = "Teleport to lobby area",
    Image = "home",
    Callback = function()
        if hrp then
            hrp.CFrame = LobbyCFrame
            AutoFarm = false
            AutoFarmToggle:SetValue(false)
            Window:Notify({
                Title = "Teleport",
                Desc = "Teleported to lobby",
                Time = 2
            })
        end
    end
})

TeleportTab:Button({
    Title = "Teleport to Game Area",
    Desc = "Teleport to game area",
    Image = "gamepad-2",
    Callback = function()
        if hrp then
            hrp.CFrame = GameAreaCFrame
            AutoFarm = false
            AutoFarmToggle:SetValue(false)
            Window:Notify({
                Title = "Teleport",
                Desc = "Teleported to game area",
                Time = 2
            })
        end
    end
})

-- Create separator using Label with empty text
TeleportTab:Label({
    Title = "",
    Desc = ""
})

TeleportTab:Section({
    Title = "Settings"
})

local TPIntervalSlider = TeleportTab:Slider({
    Title = "Teleport Interval",
    Desc = "Time between auto teleports",
    Min = 0.1,
    Max = 2,
    Rounding = 1,
    Value = TP_INTERVAL,
    Callback = function(value)
        TP_INTERVAL = value
    end
})

local MaxDistanceSlider = TeleportTab:Slider({
    Title = "Max Distance",
    Desc = "Maximum distance before teleport",
    Min = 1,
    Max = 20,
    Rounding = 0,
    Value = MAX_DISTANCE,
    Callback = function(value)
        MAX_DISTANCE = value
    end
})

-- ====== SETTINGS TAB ELEMENTS ======
SettingsTab:Section({
    Title = "Info"
})

SettingsTab:Label({
    Title = "Credits",
    Desc = "By Saul Hub"
})
-- ====== NOTIFICATIONS ======
local userId
pcall(function()
    userId = Players:GetUserIdFromNameAsync("zaluli_hrieta")
end)

local content = ""
if userId then
    local thumbType = Enum.ThumbnailType.HeadShot
    local thumbSize = Enum.ThumbnailSize.Size420x420
    content = Players:GetUserThumbnailAsync(userId, thumbType, thumbSize)
end

