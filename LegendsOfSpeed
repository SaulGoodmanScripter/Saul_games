        {"üéÆ –ò–≥—Ä–∞", "Legends of Speed"},
        {"üë§ –ò–≥—Ä–æ–∫", LocalPlayer.Name},
        {"üí∞ –ú–æ–Ω–µ—Ç—ã", "N/A"},
        {"‚ö° –°–∫–æ—Ä–æ—Å—Ç—å", "N/A"},
        {"üèÜ –£—Ä–æ–≤–µ–Ω—å", "N/A"}
    }
end

-- –û—Ç–æ–±—Ä–∞–∂–µ–Ω–∏–µ —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∏
local stats = getGameStats()
for _, stat in ipairs(stats) do
    InfoTab:Label({
        Title = stat[1],
        Desc = stat[2]
    })
end

InfoTab:Section({Title = "–°–æ—Å—Ç–æ—è–Ω–∏–µ —Å–∫—Ä–∏–ø—Ç–∞"})

