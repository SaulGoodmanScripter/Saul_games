        if character and character:FindFirstChild("HumanoidRootPart") then
            character.HumanoidRootPart.CFrame = CFrame.new(0, 10, 0)
            Window:Notify({
                Title = "üìç –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä–æ–≤–∞–Ω",
                Desc = "–ü–µ—Ä–µ–Ω–µ—Å–µ–Ω –≤ City",
                Time = 3
            })
        end
    end
})

