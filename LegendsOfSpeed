-- –ó–∞–≥—Ä—É–∂–∞–µ–º –≤–∞—à—É –±–∏–±–ª–∏–æ—Ç–µ–∫—É
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/SaulGoodmanScripter/SAUL_HUB/refs/heads/main/Library%20.lua"))()

-- –°–æ–∑–¥–∞–µ–º –≥–ª–∞–≤–Ω–æ–µ –æ–∫–Ω–æ
local Window = Library:Window({
    Title = "üéÆ Saul Hub | Legends of Speed",
    Desc = "–ê–≤—Ç–æ-—Ñ–∞—Ä–º –∏ —Ñ—É–Ω–∫—Ü–∏–∏ –¥–ª—è Legends of Speed",
    Icon = "zap", -- –ú–æ–∂–Ω–æ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞—Ç—å –≤–∞—à–∏ –∏–∫–æ–Ω–∫–∏
    Theme = "Default", -- –ò–ª–∏ "NewYear" –µ—Å–ª–∏ —Ö–æ—Ç–∏—Ç–µ –Ω–æ–≤–æ–≥–æ–¥–Ω—é—é —Ç–µ–º—É
    Config = {
        Keybind = Enum.KeyCode.RightControl,
        Size = UDim2.new(0, 500, 0, 400)
    },
    CloseUIButton = {
        Enabled = true,
        Text = "–ó–∞–∫—Ä—ã—Ç—å"
    }
})

-- –ì–ª–æ–±–∞–ª—å–Ω—ã–µ –ø–µ—Ä–µ–º–µ–Ω–Ω—ã–µ –¥–ª—è —Ñ–µ—Ä–º
local AutoOrbs = false
local AutoGems = false
local AutoSteps = false
local AutoRebirth = false
local NoClip = false
local FlyEnabled = false
