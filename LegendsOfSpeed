-- –í–ö–õ–ê–î–ö–ê –ê–í–¢–û-–§–ê–†–ú
-- ==============================================
local FarmTab = Window:Tab({Title = "‚ö° –ê–≤—Ç–æ-—Ñ–∞—Ä–º", Icon = "zap"})

FarmTab:Section({Title = "–û—Å–Ω–æ–≤–Ω—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏ —Ñ–∞—Ä–º–∞"})

-- –ü–µ—Ä–µ–º–µ–Ω–Ω–∞—è –¥–ª—è Auto Orbs
local autoOrbsToggle = FarmTab:Toggle({
    Title = "üî¥ –ê–≤—Ç–æ —Å–±–æ—Ä –æ—Ä–±–æ–≤",
    Desc = "–ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ —Å–æ–±–∏—Ä–∞–µ—Ç –≤—Å–µ —Ü–≤–µ—Ç–Ω—ã–µ –æ—Ä–±—ã",
    Value = false,
    Callback = function(value)
        AutoOrbs = value
        if value then
            spawn(function()
                while AutoOrbs do
                    task.wait(0.1)
                    -- –°–±–æ—Ä –≤—Å–µ—Ö —Ç–∏–ø–æ–≤ –æ—Ä–±–æ–≤
                    pcall(function()
                        ReplicatedStorage.rEvents.orbEvent:FireServer("collectOrb", "Orange Orb", "City")
                        ReplicatedStorage.rEvents.orbEvent:FireServer("collectOrb", "Red Orb", "City")
                        ReplicatedStorage.rEvents.orbEvent:FireServer("collectOrb", "Blue Orb", "City")
                        ReplicatedStorage.rEvents.orbEvent:FireServer("collectOrb", "Green Orb", "City")
                        ReplicatedStorage.rEvents.orbEvent:FireServer("collectOrb", "Purple Orb", "City")
                    end)
                end
            end)
        end
    end
})

-- –ü–µ—Ä–µ–º–µ–Ω–Ω–∞—è –¥–ª—è Auto Gems
local autoGemsToggle = FarmTab:Toggle({
    Title = "üíé –ê–≤—Ç–æ —Å–±–æ—Ä –≥–µ–º–æ–≤",
    Desc = "–ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ —Å–æ–±–∏—Ä–∞–µ—Ç –≥–µ–º—ã",
    Value = false,
    Callback = function(value)
        AutoGems = value
        if value then
            spawn(function()
                while AutoGems do
                    task.wait(0.5)
                    pcall(function()
                        ReplicatedStorage.rEvents.orbEvent:FireServer("collectOrb", "Gem", "City")
                    end)
                end
            end)
        end
    end
})

-- –ü–µ—Ä–µ–º–µ–Ω–Ω–∞—è –¥–ª—è Auto Steps
local autoStepsToggle = FarmTab:Toggle({
    Title = "üë£ –ê–≤—Ç–æ —Å–±–æ—Ä —à–∞–≥–æ–≤",
    Desc = "–ë—ã—Å—Ç—Ä—ã–π —Å–±–æ—Ä –¥–ª—è —É–≤–µ–ª–∏—á–µ–Ω–∏—è —à–∞–≥–æ–≤",
    Value = false,
    Callback = function(value)
        AutoSteps = value
        if value then
            spawn(function()
                while AutoSteps do
                    task.wait(1.5)
                    pcall(function()
                        for i = 1, 100 do
                            ReplicatedStorage.rEvents.orbEvent:FireServer("collectOrb", "Orange Orb", "City")
                            ReplicatedStorage.rEvents.orbEvent:FireServer("collectOrb", "Red Orb", "City")
                            ReplicatedStorage.rEvents.orbEvent:FireServer("collectOrb", "Blue Orb", "City")
                        end
                    end)
                end
            end)
        end
    end
})

-- –ü–µ—Ä–µ–º–µ–Ω–Ω–∞—è –¥–ª—è Auto Rebirth
local autoRebirthToggle = FarmTab:Toggle({
    Title = "üîÑ –ê–≤—Ç–æ –ø–µ—Ä–µ—Ä–æ–∂–¥–µ–Ω–∏–µ",
    Desc = "–ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–æ–µ –ø–µ—Ä–µ—Ä–æ–∂–¥–µ–Ω–∏–µ",
