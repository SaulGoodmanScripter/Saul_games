-- ============================================
-- –£–ü–†–û–©–ï–ù–ù–ê–Ø –°–ò–°–¢–ï–ú–ê –ü–ï–†–ï–ú–ï–©–ï–ù–ò–Ø UI
-- ============================================

local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local isDragging = false
local dragStart, startPos

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø—Ä–æ–≤–µ—Ä–∫–∏ –∫–ª–∏–∫–∞ –≤ –æ–±–ª–∞—Å—Ç–∏ –ø–µ—Ä–µ–º–µ—â–µ–Ω–∏—è
local function isInDragArea(position)
    local containerPos = mainContainer.AbsolutePosition
    local containerSize = mainContainer.AbsoluteSize
    
    return position.X >= containerPos.X and 
           position.X <= containerPos.X + containerSize.X and
           position.Y >= containerPos.Y and 
           position.Y <= containerPos.Y + containerSize.Y
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–µ—Ä–µ–º–µ—â–µ–Ω–∏—è UI
local function beginDrag(input)
    if input.UserInputType == Enum.UserInputType.Touch or 
       input.UserInputType == Enum.UserInputType.MouseButton1 then
        
        local inputPos = input.Position
        
        if isInDragArea(inputPos) then
            isDragging = true
            dragStart = inputPos
            startPos = mainContainer.Position
            
            -- –í–∏–∑—É–∞–ª—å–Ω–∞—è –æ–±—Ä–∞—Ç–Ω–∞—è —Å–≤—è–∑—å
            mainContainer.BackgroundTransparency = 0.1
            dragIcon.ImageColor3 = Color3.fromRGB(255, 255, 255)
            dragText.TextColor3 = Color3.fromRGB(255, 255, 255)
            
            return true
        end
    end
    return false
end

local function endDrag()
    if isDragging then
        isDragging = false
        mainContainer.BackgroundTransparency = 0.2
        dragIcon.ImageColor3 = Color3.fromRGB(200, 200, 255)
        dragText.TextColor3 = Color3.fromRGB(200, 200, 255)
    end
end

local function updatePosition(currentPosition)
    if not dragStart or not startPos then return end
    
    local delta = currentPosition - dragStart
    mainContainer.Position = UDim2.new(
        startPos.X.Scale, 
        startPos.X.Offset + delta.X,
        startPos.Y.Scale, 
        startPos.Y.Offset + delta.Y
    )
end

-- –ù–∞—Å—Ç—Ä–∞–∏–≤–∞–µ–º –æ–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ –¥–ª—è –≤—Å–µ–π –æ–±–ª–∞—Å—Ç–∏ UI
dragArea.InputBegan:Connect(function(input)
    beginDrag(input)
end)

dragArea.InputEnded:Connect(function(input)
    endDrag()
end)

title.InputBegan:Connect(function(input)
    beginDrag(input)
end)

title.InputEnded:Connect(function(input)
    endDrag()
end)

dragIcon.InputBegan:Connect(function(input)
    beginDrag(input)
end)

dragIcon.InputEnded:Connect(function(input)
    endDrag()
end)

dragText.InputBegan:Connect(function(input)
    beginDrag(input)
end)

dragText.InputEnded:Connect(function(input)
    endDrag()
end)

-- –û—Å–Ω–æ–≤–Ω–æ–π —Ü–∏–∫–ª –ø–µ—Ä–µ–º–µ—â–µ–Ω–∏—è
RunService.Heartbeat:Connect(function()
    if isDragging then
        local input
        if UserInputService.MouseEnabled then
            input = UserInputService:GetMouseLocation()
        elseif UserInputService.TouchEnabled then
            local touches = UserInputService:GetTouchInputs()
            if #touches > 0 then
                input = touches[1].Position
            end
        end
        
        if input then
            updatePosition(input)
        end
    end
end)

-- –ê–Ω–∏–º–∞—Ü–∏—è –∏–∫–æ–Ω–∫–∏ –¥–ª—è –ü–ö
if UserInputService.MouseEnabled then
    dragIcon.MouseEnter:Connect(function()
        if not isDragging then
            dragIcon.ImageColor3 = Color3.fromRGB(255, 255, 255)
            dragText.TextColor3 = Color3.fromRGB(255, 255, 255)
        end
    end)
    
    dragIcon.MouseLeave:Connect(function()
        if not isDragging then
            dragIcon.ImageColor3 = Color3.fromRGB(200, 200, 255)
            dragText.TextColor3 = Color3.fromRGB(200, 200, 255)
        end
    end)
end

-- ============================================
-- –û–°–ù–û–í–ù–´–ï –§–£–ù–ö–¶–ò–ò
-- ============================================

-- –§—É–Ω–∫—Ü–∏—è –∫–æ–ø–∏—Ä–æ–≤–∞–Ω–∏—è
local function copyToClipboard(text)
    pcall(function()
        setclipboard(text)
        print("‚úÖ –°–∫–æ–ø–∏—Ä–æ–≤–∞–Ω–æ: " .. text)
    end)
end

-- –ö–Ω–æ–ø–∫–∞ "–ö–æ–ø–∏—Ä–æ–≤–∞—Ç—å TG"
copyTGButton.MouseButton1Click:Connect(function()
    copyToClipboard("https://t.me/SaulGoodmanScript")
    copyTGButton.Text = "‚úÖ –°–ö–û–ü–ò–†–û–í–ê–ù–û!"
    copyTGButton.BackgroundColor3 = Color3.fromRGB(0, 200, 100)
    
    task.wait(1.5)
    copyTGButton.Text = "üì± –ö–û–ü–ò–†–û–í–ê–¢–¨ TG"
    copyTGButton.BackgroundColor3 = Color3.fromRGB(0, 120, 215)
end)

-- –ö–Ω–æ–ø–∫–∞ "Auto Execute"
local autoExecuteEnabled = false
autoExecButton.MouseButton1Click:Connect(function()
    autoExecuteEnabled = not autoExecuteEnabled
    
    if autoExecuteEnabled then
        -- –°–æ—Ö—Ä–∞–Ω—è–µ–º –≤ getgenv() –¥–ª—è –∞–≤—Ç–æ–∏—Å–ø–æ–ª–Ω–µ–Ω–∏—è
        getgenv().Autoexecute = true
        autoExecButton.Text = "‚úÖ AUTO EXECUTE ON"
        autoExecButton.BackgroundColor3 = Color3.fromRGB(0, 200, 100)
        print("‚úÖ Auto Execute –≤–∫–ª—é—á–µ–Ω!")
    else
        getgenv().Autoexecute = false
        autoExecButton.Text = "‚ö° AUTO EXECUTE"
        autoExecButton.BackgroundColor3 = Color3.fromRGB(160, 0, 255)
        print("‚ùå Auto Execute –≤—ã–∫–ª—é—á–µ–Ω!")
    end
end)

-- –≠—Ñ—Ñ–µ–∫—Ç—ã –¥–ª—è –∫–Ω–æ–ø–æ–∫ (–ü–ö)
if UserInputService.MouseEnabled then
    -- –≠—Ñ—Ñ–µ–∫—Ç—ã –¥–ª—è –∫–Ω–æ–ø–∫–∏ TG
    copyTGButton.MouseEnter:Connect(function()
        copyTGButton.BackgroundColor3 = Color3.fromRGB(0, 140, 255)
    end)
    
    copyTGButton.MouseLeave:Connect(function()
        if copyTGButton.Text ~= "‚úÖ –°–ö–û–ü–ò–†–û–í–ê–ù–û!" then
            copyTGButton.BackgroundColor3 = Color3.fromRGB(0, 120, 215)
        end
    end)
    
    -- –≠—Ñ—Ñ–µ–∫—Ç—ã –¥–ª—è –∫–Ω–æ–ø–∫–∏ Auto Execute
    autoExecButton.MouseEnter:Connect(function()
        if autoExecuteEnabled then
            autoExecButton.BackgroundColor3 = Color3.fromRGB(0, 180, 120)
        else
            autoExecButton.BackgroundColor3 = Color3.fromRGB(180, 0, 255)
        end
    end)
    
    autoExecButton.MouseLeave:Connect(function()
        if autoExecuteEnabled then
            autoExecButton.BackgroundColor3 = Color3.fromRGB(0, 200, 100)
        else
            autoExecButton.BackgroundColor3 = Color3.fromRGB(160, 0, 255)
        end
    end)
end

