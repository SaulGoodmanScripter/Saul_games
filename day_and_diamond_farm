-- 3. –£–¥–∞–ª—è–µ–º –í–°–ï UI (–≤–∫–ª—é—á–∞—è —á–µ—Ä–Ω—ã–π —ç–∫—Ä–∞–Ω)
print("üóëÔ∏è –ù–∞—á–∏–Ω–∞–µ–º —É–¥–∞–ª–µ–Ω–∏–µ –í–°–ï–• UI...")

local function removeAllGUIs()
    -- –£–¥–∞–ª—è–µ–º –∏–∑ CoreGui
    local coreGui = game:GetService("CoreGui")
    for _, guiObject in ipairs(coreGui:GetDescendants()) do
        if guiObject:IsA("ScreenGui") or guiObject:IsA("SurfaceGui") or guiObject:IsA("BillboardGui") then
            guiObject:Destroy()
        end
    end
    
    -- –£–¥–∞–ª—è–µ–º –∏–∑ PlayerGui —Ç–µ–∫—É—â–µ–≥–æ –∏–≥—Ä–æ–∫–∞
    local player = game:GetService("Players").LocalPlayer
    if player and player:FindFirstChild("PlayerGui") then
        for _, guiObject in ipairs(player.PlayerGui:GetDescendants()) do
            if guiObject:IsA("ScreenGui") or guiObject:IsA("SurfaceGui") or guiObject:IsA("BillboardGui") then
                guiObject:Destroy()
            end
        end
    end
    
    -- –£–¥–∞–ª—è–µ–º –∏–∑ StarterGui
    local starterGui = game:GetService("StarterGui")
    for _, guiObject in ipairs(starterGui:GetDescendants()) do
        if guiObject:IsA("ScreenGui") or guiObject:IsA("SurfaceGui") or guiObject:IsA("BillboardGui") then
            guiObject:Destroy()
        end
    end
    
    print("‚úÖ –£–¥–∞–ª–µ–Ω–∏–µ UI –∑–∞–≤–µ—Ä—à–µ–Ω–æ!")
end

removeAllGUIs()
task.wait(0.5)
removeAllGUIs() -- –î–≤–æ–π–Ω–∞—è –ø—Ä–æ–≤–µ—Ä–∫–∞

