    Desc = "–ü—Ä–æ—Å–º–æ—Ç—Ä –∏–Ω–≥—Ä–µ–¥–∏–µ–Ω—Ç–æ–≤ –∑–µ–ª—å—è",
    List = {"Grug", "idIot", "Nightmare", "Confusion", "Power", "Paralyzing", "Haste", "Invisibility", "Explosion", "Invincible", "Toxic", "Freeze", "Feather", "Speed", "Lethal", "Slow", "Antitoxin", "Corrupted Vine", "Field"},
    Value = "Grug",
    Callback = function(value)
        _G.SelectedPotion = value
    end
})

PotionTab:Button({
    Title = "üìã –ü–æ–∫–∞–∑–∞—Ç—å –∏–Ω–≥—Ä–µ–¥–∏–µ–Ω—Ç—ã",
    Desc = "–ü–æ–∫–∞–∑–∞—Ç—å –∏–Ω–≥—Ä–µ–¥–∏–µ–Ω—Ç—ã –≤—ã–±—Ä–∞–Ω–Ω–æ–≥–æ –∑–µ–ª—å—è",
    Callback = function()
        if _G.SelectedPotion and _G.GetPotion[_G.SelectedPotion] then
            local ingredients = table.concat(_G.GetPotion[_G.SelectedPotion], ", ")
            Window:Notify({
                Title = "–ò–Ω–≥—Ä–µ–¥–∏–µ–Ω—Ç—ã –¥–ª—è " .. _G.SelectedPotion,
                Desc = ingredients,
                Time = 5
            })
        end
    end
})

