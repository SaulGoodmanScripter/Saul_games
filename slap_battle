AutoFarmTab:Toggle({
    Title = "üëä Baller Farm",
    Desc = "–ê–≤—Ç–æ—Ñ–∞—Ä–º Baller –ø–µ—Ä—á–∞—Ç–∫–∏",
    Value = false,
    Callback = function(value)
        BallerFarm = value
        if value then
            task.spawn(SpamBaller)
        end
    end
})

AutoFarmTab:Toggle({
    Title = "üîÑ Replica Farm",
    Desc = "–ê–≤—Ç–æ—Ñ–∞—Ä–º Replica –ø–µ—Ä—á–∞—Ç–∫–∏",
    Value = false,
    Callback = function(value)
        ReplicaFarm = value
        if value then
            task.spawn(SpamReplica)
        end
    end
})

