    Title = "‚ö° Blink Farm",
    Desc = "–ê–≤—Ç–æ—Ñ–∞—Ä–º Blink –ø–µ—Ä—á–∞—Ç–∫–∏",
    Value = false,
    Callback = function(value)
        BlinkFarm = value
        if value then
            task.spawn(SpamBlink)
        end
    end
})

