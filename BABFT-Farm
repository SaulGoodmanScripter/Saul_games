Tab2:Button({
    Title = "üßà –ö–≤–µ—Å—Ç –º–∞—Å–ª–æ",
    Desc = "–í—ã–ø–æ–ª–Ω–∏—Ç—å –∫–≤–µ—Å—Ç —Å –º–∞—Å–ª–æ–º",
    Callback = function()
        task.spawn(function()
            local args = {4}
            workspace:WaitForChild("QuestMakerEvent"):FireServer(unpack(args))
            
            task.wait(0.5)
            
            local function getButterClickDetector()
                local brickColorToZone = {
                    ["New Yeller"] = "New YellerZone",
                    ["Really red"] = "Really redZone",
                    ["Magenta"] = "MagentaZone",
                    ["Camo"] = "CamoZone",
                    ["Really blue"] = "Really blueZone",
                    ["White"] = "WhiteZone",
                    ["Black"] = "BlackZone"
                }
                
                local zoneName = brickColorToZone[tostring(LocalPlayer.Team.TeamColor)]
                if zoneName then
                    local zone = workspace:FindFirstChild(zoneName)
                    if zone then
                        local quest = zone:FindFirstChild("Quest")
                        if quest then
                            local butter = quest:FindFirstChild("Butter")
                            if butter then
                                local ppart = butter:FindFirstChild("PPart")
                                if ppart then
                                    return ppart:FindFirstChildOfClass("ClickDetector")
                                end
                            end
                        end
                    end
                end
                return nil
            end
            
            local failCount = 0
            while failCount < 3 do
                local cd = getButterClickDetector()
                if cd then
                    fireclickdetector(cd)
                    failCount = 0
                else
                    failCount = failCount + 1
                end
                task.wait(0.5)
            end
        end)
    end
})

Tab2:Button({
    Title = "‚öΩ –§—É—Ç–±–æ–ª –∫–≤–µ—Å—Ç",
    Desc = "–í—ã–ø–æ–ª–Ω–∏—Ç—å –∫–≤–µ—Å—Ç —Ñ—É—Ç–±–æ–ª",
    Callback = function()
        task.spawn(function()
            local teamBlocks = {
                green = {
                    zone = "CamoZone",
                    placeCF = CFrame.new(49, 6.1, -27),
                    targetCF = CFrame.new(-355.9657, -11.89999, 222.89316, 0, 0, 1, 0, 1, 0, -1, 0, 0)
                },
                magenta = {
                    zone = "MagentaZone",
                    placeCF = CFrame.new(49, 6.1, -27),
                    targetCF = CFrame.new(248.83432, -11.89999, 710.69318, 0, 0, -1, 0, 1, 0, 1, 0, 0)
                },
                blue = {
                    zone = "Really blueZone",
                    placeCF = CFrame.new(49, 6.1, -27),
                    targetCF = CFrame.new(248.83432, -11.89999, 352.49319, 0, 0, -1, 0, 1, 0, 1, 0, 0)
                },
                red = {
                    zone = "Really redZone",
                    placeCF = CFrame.new(49, 6.1, -28),
                    targetCF = CFrame.new(249.83432, -11.89999, -5.70679, 0, 0, -1, 0, 1, 0, 1, 0, 0)
                },
                black = {
                    zone = "BlackZone",
                    placeCF = CFrame.new(48.99999, 6.1, -27),
                    targetCF = CFrame.new(-355.9657, -11.89999, -136.10684, 0, 0, 1, 0, 1, 0, -1, 0, 0)
                },
                white = {
                    zone = "WhiteZone",
                    placeCF = CFrame.new(49, 6.1, -27),
                    targetCF = CFrame.new(9.43431, -11.89999, -372.50687, 1, 0, 0, 0, 1, 0, 0, 0, 1)
                },
                yellow = {
                    zone = "New YellerZone",
                    placeCF = CFrame.new(49, 6.1, -27),
                    targetCF = CFrame.new(-355.9657, -11.89999, 580.89319, 0, 0, 1, 0, 1, 0, -1, 0, 0)
                }
            }
            
            local function findTeamData(teamName)
                if not teamName then return nil end
                local nameLower = string.lower(teamName)
                for keyword, data in pairs(teamBlocks) do
                    if string.find(nameLower, keyword) then
                        return data
                    end
                end
                return nil
            end
            
            local teamName = LocalPlayer.Team and LocalPlayer.Team.Name or nil
            local data = findTeamData(teamName)
            
            if data then
                workspace:WaitForChild("QuestMakerEvent"):FireServer(8)
                
                local tool = LocalPlayer.Character:FindFirstChild("BuildingTool") or LocalPlayer.Backpack:FindFirstChild("BuildingTool")
                while not tool do
                    LocalPlayer.CharacterAdded:Wait()
                    tool = LocalPlayer.Character:FindFirstChild("BuildingTool") or LocalPlayer.Backpack:FindFirstChild("BuildingTool")
                end
                
                local args = {
                    "WoodBlock",
                    9,
                    workspace:WaitForChild(data.zone),
                    data.placeCF,
                    true,
                    data.targetCF,
                    false
                }
                tool:WaitForChild("RF"):InvokeServer(unpack(args))
                
                workspace:WaitForChild("ClearAllPlayersBoatParts"):FireServer()
            end
        end)
    end
})

-- –í–∫–ª–∞–¥–∫–∞ Auto Buy
local Tab3 = Window:Tab({Title = "üõí Auto Buy", Icon = "shopping-cart"})

Tab3:Section({Title = "üì¶ –°—É–Ω–¥—É–∫–∏"})

local chests = {
    {name = "–û–±—ã—á–Ω—ã–π", item = "Common Chest", delay = 3},
    {name = "–ù–µ–æ–±—ã—á–Ω—ã–π", item = "Uncommon Chest", delay = 3},
    {name = "–†–µ–¥–∫–∏–π", item = "Rare Chest", delay = 3},
    {name = "–≠–ø–∏—á–µ—Å–∫–∏–π", item = "Epic Chest", delay = 3},
    {name = "–õ–µ–≥–µ–Ω–¥–∞—Ä–Ω—ã–π", item = "Legendary Chest", delay = 3}
}

for _, chest in ipairs(chests) do
    local running = false
    local button = Tab3:Button({
        Title = "üì¶ " .. chest.name .. " —Å—É–Ω–¥—É–∫",
        Desc = "–ê–≤—Ç–æ–ø–æ–∫—É–ø–∫–∞ " .. chest.name .. " —Å—É–Ω–¥—É–∫–∞",
        Callback = function()
            running = not running
            
            if running then
                button:SetTitle("üõë –û—Å—Ç–∞–Ω–æ–≤–∏—Ç—å " .. chest.name)
                
                task.spawn(function()
                    while running do
                        local args = {chest.item, 1}
                        pcall(function()
                            workspace:WaitForChild("ItemBoughtFromShop"):InvokeServer(unpack(args))
                        end)
                        task.wait(chest.delay)
                    end
                    
                    button:SetTitle("üì¶ " .. chest.name .. " —Å—É–Ω–¥—É–∫")
                end)
            else
                button:SetTitle("üì¶ " .. chest.name .. " —Å—É–Ω–¥—É–∫")
            end
        end
    })
end

Tab3:Section({Title = "üß± –ë–ª–æ–∫–∏"})

Tab3:Button({
    Title = "üîÑ –ü–ª–∞—Å—Ç–∏–∫–æ–≤—ã–π –±–ª–æ–∫",
    Desc = "–ê–≤—Ç–æ–ø–æ–∫—É–ø–∫–∞ –ø–ª–∞—Å—Ç–∏–∫–æ–≤—ã—Ö –±–ª–æ–∫–æ–≤",
    Callback = function()
        local running = true
        
        task.spawn(function()
            while running do
                local args = {"PlasticBlock", 1}
                pcall(function()
                    workspace:WaitForChild("ItemBoughtFromShop"):InvokeServer(unpack(args))
                end)
                task.wait(3)
            end
        end)
    end
})

