-- –í–∫–ª–∞–¥–∫–∞ Auto Quest
local Tab2 = Window:Tab({Title = "üéØ Auto Quest", Icon = "target"})

Tab2:Section({Title = "üìã –ö–≤–µ—Å—Ç—ã"})

Tab2:Button({
    Title = "‚òÅÔ∏è –ö–≤–µ—Å—Ç –æ–±–ª–∞–∫–æ",
    Desc = "–í—ã–ø–æ–ª–Ω–∏—Ç—å –∫–≤–µ—Å—Ç —Å –æ–±–ª–∞–∫–æ–º",
    Callback = function()
        local function startQuest()
            local args = {1}
            workspace:WaitForChild("QuestMakerEvent"):FireServer(unpack(args))
        end
        
        local function touchCloud()
            local brickColorToCloud = {
                ["Magenta"] = function() return workspace["MagentaZone"].Quest.Cloud.Part1 end,
                ["Really red"] = function() return workspace["Really redZone"].Quest.Cloud.Part1 end,
                ["Camo"] = function() return workspace["CamoZone"].Quest.Cloud.Part1 end,
                ["Really blue"] = function() return workspace["Really blueZone"].Quest.Cloud.Part1 end,
                ["Black"] = function() return workspace["BlackZone"].Quest.Cloud.Part1 end,
                ["White"] = function() return workspace["WhiteZone"].Quest.Cloud.Part1 end,
                ["New Yeller"] = function() return workspace["New YellerZone"].Quest.Cloud.Part1 end
            }
            
            local brickColorName = tostring(LocalPlayer.Team.TeamColor)
            local cloudGetter = brickColorToCloud[brickColorName]
            
            if cloudGetter then
                local cloudPart = cloudGetter()
                if cloudPart and cloudPart:FindFirstChild("TouchInterest") then
                    firetouchinterest(LocalPlayer.Character.HumanoidRootPart, cloudPart, 0)
                    task.wait(0.1)
                    firetouchinterest(LocalPlayer.Character.HumanoidRootPart, cloudPart, 1)
                end
            end
        end
        
        startQuest()
        task.wait(0.5)
        touchCloud()
    end
})

Tab2:Button({
    Title = "üéØ –ö–≤–µ—Å—Ç –º–∏—à–µ–Ω—å",
    Desc = "–í—ã–ø–æ–ª–Ω–∏—Ç—å –∫–≤–µ—Å—Ç —Å –º–∏—à–µ–Ω—å—é",
    Callback = function()
        local function startQuest()
            local args = {2}
            workspace:WaitForChild("QuestMakerEvent"):FireServer(unpack(args))
        end
        
        local function touchTarget()
            local brickColorToTarget = {
                ["Magenta"] = function() return workspace["MagentaZone"].Quest.Target.Part end,
                ["Really red"] = function() return workspace["Really redZone"].Quest.Target.Part end,
                ["Camo"] = function() return workspace["CamoZone"].Quest.Target.Part end,
                ["Really blue"] = function() return workspace["Really blueZone"].Quest.Target.Part end,
                ["Black"] = function() return workspace["BlackZone"].Quest.Target.Part end,
                ["White"] = function() return workspace["WhiteZone"].Quest.Target.Part end,
                ["New Yeller"] = function() return workspace["New YellerZone"].Quest.Target.Part end
            }
            
            local brickColorName = tostring(LocalPlayer.Team.TeamColor)
            local targetGetter = brickColorToTarget[brickColorName]
            
            if targetGetter then
                local targetPart = targetGetter()
                if targetPart and targetPart:FindFirstChild("TouchInterest") then
                    firetouchinterest(LocalPlayer.Character.HumanoidRootPart, targetPart, 0)
                    task.wait(0.1)
                    firetouchinterest(LocalPlayer.Character.HumanoidRootPart, targetPart, 1)
                end
            end
        end
        
        startQuest()
        task.wait(0.5)
        touchTarget()
    end
})

Tab2:Button({
    Title = "üõë –ö–≤–µ—Å—Ç —Ä–∞–º–ø–∞",
    Desc = "–í—ã–ø–æ–ª–Ω–∏—Ç—å –∫–≤–µ—Å—Ç —Å —Ä–∞–º–ø–æ–π",
