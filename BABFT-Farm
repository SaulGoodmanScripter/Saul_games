Tab1:Button({
    Title = "üßπ –û—á–∏—Å—Ç–∏—Ç—å –ø–ª–∞—Ç—Ñ–æ—Ä–º—ã",
    Desc = "–£–¥–∞–ª–∏—Ç—å –≤—Å–µ —Å–æ–∑–¥–∞–Ω–Ω—ã–µ –ø–ª–∞—Ç—Ñ–æ—Ä–º—ã",
    Callback = function()
        removePlatforms()
        Window:Notify({
            Title = "üßπ –û—á–∏—â–µ–Ω–æ",
            Desc = "–í—Å–µ –ø–ª–∞—Ç—Ñ–æ—Ä–º—ã —É–¥–∞–ª–µ–Ω—ã",
            Time = 3
        })
    end
})

